package com.mycompany.xtravisionmovierentalsystem;

/**
 *
 * @author johnWick
 */
public class Movie {
    private int id;
    private double rating;
    private String title;
    private String description;

    public Movie(int id, double rating, String title, String description){
        this.id = id;
        this.rating = rating;
        this.title = title;
        this.description = description;
    }
    
    public int getId(){
        return id;
    }
    public double getRating(){
        return rating;
    }
    public String getTitle(){
        return title;
    }
     public String getDescription(){
        return description;
    }
  
}
