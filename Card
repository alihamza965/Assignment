        package com.mycompany.xtravisionmovierentalsystem;

/**
 *
 * @author johnWick
 */
public class CardDetail {
    private String cardNo, expirydate, title, cvv;
    public CardDetail(String cardNo,String expirydate,String title,String cvv){
        this.cardNo = cardNo;
        this.expirydate = expirydate;
        this.title = title;
        this.cvv = cvv;
    }

    public String getCardNo(){
        return cardNo;
    }
    public String getExpirydate(){
        return expirydate;
    }
    public String getTitle(){
        return title;
    }
    public String getCvv(){
        return cvv;
    }
}
